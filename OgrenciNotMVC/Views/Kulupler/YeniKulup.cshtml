
@{
    ViewBag.Title = "YeniKulup";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>YeniKulup</h2>

<form method="post" class="form-group">
    <div>
        <label>Kulüp Adı</label>
        <input type="text" class="form-control" name="KULUPAD" />
    </div>

    <div>
        <label>Kulüp Kontenjanı</label>
        <input type="number" class="form-control" name="KULUPKONTENJAN" />
    </div>

    <div style="margin-top:10px">
        <button class="btn btn-info"> Kulüp Ekle</button>

    </div>

</form>