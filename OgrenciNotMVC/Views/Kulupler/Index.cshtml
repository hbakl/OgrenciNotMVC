@using OgrenciNotMVC.Models.EntityFramework
@model List<TBLKULUPLER>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>Kulüp ID</th>
        <th>Kulüp Adı</th>
    </tr>
    <tbody>
        @foreach (var klp in Model)
        {
            <tr>
                <td> @klp.KULUPID </td>
                <td> @klp.KULUPAD </td>
            </tr>
        }



    </tbody>
</table>

<a href="/Kulupler/YeniKulup" class="btn btn-primary">Kulüp Ekle</a>